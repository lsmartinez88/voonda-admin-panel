# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Sheets Configuration
GOOGLE_SHEETS_ID=your_google_sheets_id
GOOGLE_API_KEY=your_google_api_key
# API Key para leer Google Sheets desde el frontend (necesaria para sincronización de catálogo)
VITE_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key

# Google Service Account (for write access to sheets)
GOOGLE_CLIENT_EMAIL=your_service_account_email
GOOGLE_PRIVATE_KEY=your_service_account_private_key

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_KEY=your_api_key

# Google Maps
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key

# Environment
VITE_APP_ENV=development
NODE_ENV=development

# Application Settings
VITE_APP_NAME=Voonda Admin Panel
VITE_APP_VERSION=1.0.0

# Ejemplos:
# VITE_SUPABASE_URL=https://xxxxxxxxxxx.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# VITE_OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxxxxxxx